{
  "name": "coginivise",
  "displayName": "coginivise",
  "description": "VS Code extension to enhance developer prompts and detect/fix insecure code patterns.",
  "version": "1.0.0",
  "publisher": "coginivise",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Linters",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:json"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext .ts",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.2.5",
    "@types/vscode": "^1.83.0",
    "eslint": "^8.44.0",
    "typescript": "^5.1.3",
    "vsce": "^2.11.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "groq-sdk": "^0.32.0",
    "node-fetch": "^2.7.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "coginivise.runScan",
        "title": "coginivise: Run Security Scan"
      }
    ],
    "codeActions": [
      {
        "languages": [
          "javascript",
          "typescript",
          "json"
        ],
        "kind": "quickfix"
      }
    ]
  },
  "keywords": [
    "vscode",
    "security",
    "lint",
    "safe-coding",
    "semgrep",
    "RAG"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/coginivise/coginivise.git"
  },
  "homepage": "https://github.com/coginivise/coginivise#readme",
  "bugs": {
    "url": "https://github.com/coginivise/coginivise/issues"
  },
  "license": "MIT"
}